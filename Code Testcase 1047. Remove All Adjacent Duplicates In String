class Solution {
    public String removeDuplicates(String s) {
       Stack<Character> ch = new Stack<>();
       StringBuilder sb = new StringBuilder();
       for(int i=0;i<s.length();i++){
        char c = s.charAt(i);
        if(!ch.isEmpty()&&ch.peek()==c){
            ch.pop();
        }

        else{
            ch.push(c);
        }
    }
      for(char C:ch){
            sb.append(C);
        }
        return sb.toString();
    }
}
