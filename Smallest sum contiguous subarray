// User function Template for Java

class Solution {
    static int smallestSumSubarray(int a[], int size) {
        // your code here
        int sum=a[0];
        int min=a[0];
        for(int i=1;i<size;i++){
            if(sum>0){
                sum=a[i];
            }
            else{
                sum+=a[i];
            }
            min=Math.min(min,sum);
            
        }
        return min;
    }
}
