class Solution {
    public int subarraySum(int[] nums, int k) {
        int sum=0;
        int res=0;
        Map<Integer,Integer> m=new HashMap<>();
        m.put(0,1);
        for(int i=0;i<nums.length;i++){
            sum+=nums[i];
            int need=sum-k;
            res+=m.getOrDefault(need,0);

            m.put(sum,m.getOrDefault(sum,0)+1);
        }
        return res;
    }
}
